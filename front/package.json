  "private": true,
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>enzyme.config.js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "transform": {
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.(js|jsx)?$": "babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ]
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server -d --hot --config webpack/webpack.dev.config.js --watch",
    "analyzer": "rimraf ./dist && cross-env NODE_ENV=production webpack --config webpack/webpack.analyzer.config.js",
    "build": "rimraf ./dist && cross-env NODE_ENV=production webpack --config webpack/webpack.prod.config.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
